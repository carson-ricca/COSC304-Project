<h4>Customer Profile</h4>
{{#if failed}}
<div class="alert alert-danger">{{failed}}</div>
{{/if}}
<table class="table table-bordered">
   <thead>
       <tr>
            <th>Id</th>
            <th>First Name</th>
            <th>Last Name</th>
            <th>Email</th>
            <th>Phone</th>
            <th>Address</th>
            <th>City</th>
            <th>State</th>
            <th>Postal Code</th>
            <th>Country</th>
            <th>User id</th>
        </tr>
    </thead>
    <tbody>
        {{#each info}}
            <tr>
                <td>{{customerId}}</td>
                <td>{{firstName}}</td>
                <td>{{lastName}}</td>
                <td>{{email}}</td>
                <td>{{phonenum}}</td>
                <td>{{address}}</td>
                <td>{{city}}</td>
                <td>{{state}}</td>
                <td>{{postalCode}}</td>
                <td>{{country}}</td>
                <td>{{userid}}</td>
            </tr>
        {{/each}}
    </tbody>
</table>
